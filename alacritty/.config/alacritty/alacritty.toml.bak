[general]
import = ["/home/ryewell/.cache/wal/colors-alacritty.toml"]
# BEGIN WAL
[colors.primary]
background = '#0A0D12'
foreground = '#a0e8f3'

[colors.cursor]
text   = '#0A0D12' # Apply background color to color of text inside cursor
cursor = '#a0e8f3' # Apply foreground color to color of cursor

[colors.vi_mode_cursor]
text    = '#0A0D12' # Same as above
cursor  = '#a0e8f3'

[colors.search.matches]
foreground = '#0A0D12' # Same as above
background = '#a0e8f3' # Apply bright/white color to bg color of matching search

[colors.search.focused_match]
foreground = 'CellBackground'
background = 'CellForeground'

[colors.line_indicator]
foreground = 'None'
background = 'None'

[colors.footer_bar]
foreground = '#70a2aa'
background = '#a0e8f3'

[colors.selection]
text       = 'CellBackground'
background = 'CellForeground'

[colors.normal]
black   = '#0A0D12'
red     = '#63199A'
green   = '#3C43AC'
yellow  = '#A427CA'
blue    = '#E018EE'
magenta = '#39A6CA'
cyan    = '#2FE0EE'
white   = '#a0e8f3'

[colors.bright]
black   = '#70a2aa'
red     = '#63199A'
green   = '#3C43AC'
yellow  = '#A427CA'
blue    = '#E018EE'
magenta = '#39A6CA'
cyan    = '#2FE0EE'
white   = '#a0e8f3'
# END WAL
